<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.City" table="city" schema="jdata">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="cityname">
            <column name="cityname" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="tip">
            <column name="tip" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="num">
            <column name="num" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>